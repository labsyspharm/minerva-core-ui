{
  "compilerOptions": {
    "noImplicitAny": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "react",
    "allowJs": true,
    "strict": false,
    "outDir": "./build",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "esnext",
      "DOM",
      "DOM.Iterable"
    ]
  },
  "include": [
    "src"
  ]
}